<!doctype html>
<html>
  <head>
    <meta charset ="uth-8">
    <title>learning Object</title>
    <script>
      function fam(self){
              document.write((self).toString() + " : " + coworkers[self] + "<br>");
        }
    </script>

  </head>
  <body>
    <h1>Object</h1>
    <h2>Create</h2>
    <script>
      
      var coworkers = {
        "me" : "jh",
        "sis" : "mj"
      };
      
      fam("me");
      fam("sis");
      
      coworkers.mom = "yang";
      fam("mom");
      coworkers["happy dad"] = "jong";
      fam("happy dad")

      <!--coworkers라는 객체에 jh라는 정보를 me라는 딱지를 붙여서 저장한 것-->
    </script>
  </body>
</html>
