    <script>
      
      var coworkers = {
        "me" : "jh",
        "sis" : "mj"
      };
            <!--coworkers라는 객체에 jh라는 정보를 me라는 딱지를 붙여서 저장한 것-->
      coworkers.mom = "yang";
      coworkers["happy dad"] = "jong";

      for (var property in coworkers){
        do
      }
      
      /*
      fam("me");
